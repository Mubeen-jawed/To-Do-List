<style>body {
  background-color: #E4E9FD;
  background-image: -webkit-linear-gradient(65deg, #A683E3 50%, #E4E9FD 50%);
}</style>


<body class="d-flex align-items-center py-4 bg-body-tertiary" data-new-gr-c-s-check-loaded="14.1112.0" data-gr-ext-installed="">
  <main class="form-signin d-flex align-items-center w-100 justify-content-center" style="height: 100vh;">
    <form class="w-25" action="/login" method="post">
      <h1 style="color: #7424ff; font-family: helvetica neue;" class="h1 mb-3 text-center">Log In</h1>
      <p class="text-danger text-center d-none error-message">User Don't Exist</p>
      <div class="form-floating mt-2">
        <input name="loginEmail" type="email" id="input-3" class="form-control" id="floatingInput" autocomplete="on" placeholder="name@example.com">
        <label for="floatingInput">Email address</label>
      </div>
      <div class="form-floating mt-2">
        <input name="loginPassword" type="password" id="input-4" class="form-control" id="floatingPassword" autocomplete="on" placeholder="Password">
        <label for="floatingPassword">Password</label>
      </div>
  
      <button style="background-color: #7424ff; font-weight: 700;" id="SignUp" class="mt-4 w-100 text-light btn btn-lg signup-button" type="submit"  >Log In</button>
      <p class="mt-5 mb-3 text-center">Don't Have An Account? <a href="/signup">Sign Up</a></p>
    </form>
  </main>
<script src="/docs/5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous"></script>

  

